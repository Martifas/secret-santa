{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npx tsc-alias",
    "coverage": "vitest run --coverage",
    "dev": "tsx watch src",
    "check-deps": "depcheck",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate:new": "tsx ./src/database/migrate/new",
    "migrate:latest:prod": "npm run node-es -- dist/database/migrate/latest.js",
    "migrate:latest": "tsx ./src/database/migrate/latest",
    "gen:types": "kysely-codegen --camel-case --dialect postgres --out-file src/database/types.ts && prettier --write src/database/types.ts",
    "node-es": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"./loader.js\", pathToFileURL(\"./\"));'",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "prod": "npm run migrate:latest:prod && npm run start",
    "start": "npm run node-es ./dist/index.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/chance": "^1.1.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitest/coverage-v8": "^2.1.8",
    "chance": "^1.1.12",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "kysely-codegen": "^0.17.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@trpc/server": "^10.45.2",
    "@types/lodash-es": "^4.17.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-openid-connect": "^2.17.1",
    "kysely": "^0.27.5",
    "lodash-es": "^4.17.21",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "superjson": "^2.2.2",
    "trpc-panel": "^1.3.4",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  }
}
